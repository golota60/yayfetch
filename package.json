{
  "name": "yayfetch",
  "version": "2.0.2",
  "description": "A Multi-platform screenfetch",
  "main": "yayfetch.js",
  "author": "Szymon Wiszczuk",
  "license": "MIT",
  "keywords": [
    "screenfetch",
    "neofetch",
    "environment",
    "info",
    "system",
    "sysinf",
    "systeminformation"
  ],
  "files": [
    "lib"
  ],
  "publishConfig": {
    "access": "public",
    "directory": "lib"
  },
  "homepage": "https://github.com/golota60/yayfetch#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/golota60/yayfetch"
  },
  "scripts": {
    "build": "rimraf lib && tsc",
    "start": "yarn build && node ./build/yayfetch.js",
    "clean": "rm -rf node_modules && rm -rf ./build",
    "lint": "yarn eslint --ext '.js,.ts' ./src --fix",
    "jest": "jest tests",
    "test": "yarn start && yarn lint && yarn jest",
    "prepare": "husky install",
    "prepublishOnly": "yarn && yarn build",
    "release": "standard-version"
  },
  "hooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*": "yarn test"
  },
  "bin": {
    "yayfetch": "./yayfetch.js"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "inquirer": "^7.0.0",
    "log-update": "^4.0.0",
    "systeminformation": "^5.6.4",
    "terminal-image": "^1.2.1",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@callstack/eslint-config": "^11.0.0",
    "@types/getos": "^3.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.0",
    "@types/yargs": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "lint-staged": "^11.0.0",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2",
    "standard-version": "^9.3.2",
    "ts-jest": "^26.5.6",
    "ts-node": "^8.5.0",
    "typescript": "^4.2.4"
  }
}
