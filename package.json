    "systeminformation": "^4.14.17",
    "yargs": "^14.2.0"
  },
  "devDependencies": {
    "@types/inquirer": "^6.5.0",
    "@types/node": "^12.12.7",
    "@types/yargs": "^13.0.3",
    "ts-node": "^8.5.0",
    "typescript": "^3.0.3"
  }
}
